int[7] num3;
int temp;
int sv;
int i;
int k;
int mid;
int low;
int high;

def void binsea() 
{
	low = 0;
	high = 6;
	while (low <= high) {
		mid = (high+low)/2;
		if (sv < num3[mid]) {
			high = mid - 1;
		}
		else {
			if ( sv > num3[mid]) {
				low = mid + 1;
			}
			else {
				low = high + 1;
			}
		}
	}
}

def void sort() 
{
	i = 0;
	while ( i < 10) {
		k = 0;
                while (k < 6 - i) {
                        if (num3[k] > num3[k+1]) {
                                temp = num3[k];
                                num3[k] = num3[k+1];
                                num3[k+1] = temp;
                        }
                        k= k+1;
                }
		i = i+1;
	}
}

def void printn() 
{
	i = 0;
	while ( i < 7) {
		print num3[i];
		i = i+1;
	}
}

def void readnu() 
{
	i = 0;
	while (i < 7) {
		num3[i] = read;
		i = i+1;
	}
}

readnu();
sort();
printn();
sv = read;
binsea();
print mid;



